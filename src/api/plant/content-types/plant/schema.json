{
  "kind": "collectionType",
  "collectionName": "plants",
  "info": {
    "singularName": "plant",
    "pluralName": "plants",
    "displayName": "Plant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Type": {
      "type": "string",
      "required": true
    },
    "Streak": {
      "type": "integer",
      "required": false,
      "default": 1,
      "min": 0
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Alive": {
      "type": "boolean",
      "default": true
    },
    "Hydrated": {
      "type": "boolean",
      "required": false,
      "default": true
    },
    "Temperature": {
      "type": "decimal",
      "default": 20
    },
    "Light": {
      "type": "decimal",
      "default": 0.5,
      "required": false
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::owner.owner",
      "inversedBy": "plants"
    },
    "device_id": {
      "type": "string",
      "required": true,
      "maxLength": 4,
      "minLength": 4,
      "unique": true
    }
  }
}
